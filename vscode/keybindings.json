[
  {
    "key": "alt+shift+t",
    "command": "trailing-spaces.deleteTrailingSpaces",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+k z",
    "command": "extension.multiCommand.execute",
    "args": {
      "command": "multiCommand.betterZenMode"
    }
  },
  {
    "key": "ctrl+k f",
    "command": "editor.action.codeAction",
    "args": {
      "kind": "source.fixAll.tslint",
      "apply": "first"
    }
  },
  {
    "key": "ctrl+j",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetVisible"
  },
  {
    "key": "ctrl+k",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetVisible"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.togglePanel",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+f w",
    "command": "actions.find",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "ctrl+shift+b",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+f ctrl+f",
    "command": "workbench.action.findInFiles"
  },
  {
    "key": "ctrl+f f",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+f t",
    "command": "workbench.action.showAllSymbols"
  },
  {
    "key": "ctrl+shift+space",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
  },
  {
    "key": "shift+alt+down",
    "command": "editor.action.copyLinesDownAction"
  },
  {
    "key": "shift+alt+up",
    "command": "editor.action.copyLinesUpAction"
  },
  {
    "key": "ctrl+[",
    "command": "editor.action.outdentLines"
  },
  {
    "key": "ctrl+]",
    "command": "editor.action.indentLines"
  },
  {
    "key": "ctrl+d",
    "command": "editor.action.addSelectionToNextFindMatch"
  },
  {
    "key": "ctrl+shift+/",
    "command": "breadcrumbs.focus",
    "when": "breadcrumbsPossible && breadcrumbsVisible"
  },
  {
    "key": "ctrl+shift+;",
    "command": "-breadcrumbs.focus",
    "when": "breadcrumbsPossible && breadcrumbsVisible"
  },
  {
    "key": "ctrl+shift+g ctrl+shift+t",
    "command": "go.add.tags"
  },
  {
    "key": "a",
    "command": "explorer.newFile",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "key": "backspace",
    "command": "deleteFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceMoveableToTrash && !explorerResourceReadonly && !inputFocus && !treeFindOpen"
  },
  {
    "key": "d",
    "command": "deleteFile",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "key": "h",
    "command": "workbench.files.action.collapseExplorerFolders",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "key": "l",
    "command": "list.expand",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "key": " ",
    "command": "list.expand",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "key": "j",
    "command": "list.focusDown",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "key": "k",
    "command": "list.focusUp",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "key": "ctrl+backspace ctrl+backspace",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+f4",
    "command": "-workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+space e",
    "command": "workbench.view.explorer",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+crtl+f",
    "command": "-workbench.view.explorer",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+space e",
    "command": "workbench.action.closeSidebar",
    "when": "explorerViewletFocus && !inputFocus"
  },
  {
    "key": "ctrl+space f",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
  },
  {
    "key": "shift+alt+f",
    "command": "-editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
  }
]
